{
  "expo": {
    "name": "qeazev5",
    "slug": "qeazev5",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/applogo.jpeg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sabeelnahmed.qeazev5"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/applogo.jpeg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sabeelnahmed.qeazev5",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker"
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1bd45983-dfde-4602-ac69-de7c718b474b"
      }
    }
  }
}
